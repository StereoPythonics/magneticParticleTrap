<Window x:Class="simulationTemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:simulationTemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="DimGray">
    <Grid>
        <Image Source="{Binding back}"/>
        <Image Source="{Binding test}" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <TextBlock Text="{Binding Readout}" Foreground="Red"/>
    </Grid>
</Window>
